{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "indent": "off",
        "linebreak-style":"off",
        "no-trailing-spaces":"off",
        "object-curly-spacing":"off",
        "space-before-blocks":"off",
        "semi":"off",
        "no-unused-vars":"off",
        
    }
}
